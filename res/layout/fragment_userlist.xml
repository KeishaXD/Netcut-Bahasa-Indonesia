<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:padding="0.0dip"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <LinearLayout
            android:orientation="vertical"
            android:id="@id/nodeview2"
            android:background="@color/bgd3"
            android:padding="0.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            xmlns:android="http://schemas.android.com/apk/res/android">
            <LinearLayout
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:id="@id/totalspeedcontrol"
                android:background="@color/item_color3"
                android:layout_width="fill_parent"
                android:layout_height="40.0dip">
                <ImageButton
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:id="@id/img_totalcut"
                    android:background="#00000000"
                    android:layout_width="0.0dip"
                    android:layout_height="20.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:src="@drawable/totalspeed0"
                    android:layout_weight="0.1" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.8">
                    <SeekBar
                        android:layout_gravity="center"
                        android:id="@id/seekbar_speed_limit"
                        android:layout_width="fill_parent"
                        android:layout_height="20.0dip"
                        android:progressDrawable="@drawable/seekbar_progress"
                        android:thumb="@drawable/seekbar_new" />
                    <TextView
                        android:textSize="8.0sp"
                        android:typeface="sans"
                        android:textStyle="bold"
                        android:textColor="#ffffffff"
                        android:gravity="center|top"
                        android:id="@id/seekbar_status"
                        android:visibility="gone"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5.0dip"
                        android:layout_marginBottom="5.0dip"
                        android:text="Kecepatan saat ini: 100%" />
                </LinearLayout>
                <ImageButton
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:id="@id/img_fullacces"
                    android:background="#00000000"
                    android:layout_width="0.0dip"
                    android:layout_height="20.0dip"
                    android:layout_marginRight="5.0dip"
                    android:src="@drawable/totalspeed100"
                    android:layout_weight="0.1" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:padding="0.0dip"
                android:layout_width="fill_parent"
                android:layout_height="40.0dip"
                android:layout_marginTop="5.0dip"
                android:weightSum="5.0"
                xmlns:android="http://schemas.android.com/apk/res/android">
                <View
                    android:layout_width="0.0dip"
                    android:layout_height="0.0dip"
                    android:layout_weight="0.05" />
                <ToggleButton
                    android:textSize="10.0sp"
                    android:typeface="sans"
                    android:textStyle="bold"
                    android:textColor="#ffffffff"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:id="@id/button_protect"
                    android:background="@drawable/toggle_bg"
                    android:layout_width="0.0dip"
                    android:layout_height="30.0dip"
                    android:textOn="Protected"
                    android:textOff="Not Protected"
                    android:layout_weight="0.9" />
                <View
                    android:layout_width="0.0dip"
                    android:layout_height="0.0dip"
                    android:layout_weight="0.05" />
                <ImageButton
                    android:gravity="left"
                    android:layout_gravity="left"
                    android:id="@id/img_locknet"
                    android:background="#00ffffff"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:src="@drawable/net_unlock"
                    android:layout_weight="1.0" />
                <ImageButton
                    android:gravity="left"
                    android:layout_gravity="left"
                    android:id="@id/img_totalspeed"
                    android:background="#00ffffff"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:src="@drawable/totalspeed100"
                    android:layout_weight="1.0" />
                <ImageButton
                    android:gravity="left"
                    android:layout_gravity="left"
                    android:id="@id/img_scan"
                    android:background="#00ffffff"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:src="@drawable/scan"
                    android:layout_weight="1.0" />
                <ImageButton
                    android:gravity="left"
                    android:layout_gravity="left"
                    android:id="@id/img_search"
                    android:background="#00ffffff"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:src="@drawable/search"
                    android:layout_weight="1.0" />
            </LinearLayout>
        </LinearLayout>
        <com.lrcai.ravens.JExpandListView.JListViewBase
            android:id="@id/nodeview"
            android:background="@color/bgd1"
            android:padding="0.0dip"
            android:paddingTop="5.0dip"
            android:paddingBottom="5.0dip"
            android:animationCache="false"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:scrollingCache="false"
            android:divider="#00ffffff"
            android:dividerHeight="0.0px"
            android:layout_weight="1.0"
            android:layout_above="@id/nodeview2"
            android:layout_alignParentTop="true"
            android:fastScrollEnabled="true"
            android:fastScrollAlwaysVisible="false" />
        <View
            android:background="@drawable/shadow_bb"
            android:layout_width="fill_parent"
            android:layout_height="5.0dip"
            android:layout_above="@id/nodeview2" />
    </RelativeLayout>
</LinearLayout>